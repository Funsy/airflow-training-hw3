create external table ashanin.stg_payment (user_id bigint, 
      pay_doc_type text, 
      pay_doc_num bigint, 
      account text, 
      phone text, 
      billing_period date, 
      pay_date date, 
      sum numeric(10,2))
  location ('pxf://rt-2021-03-25-16-47-29-sfunu-ashanin/data_lake/stg/payment/*/?PROFILE=gs:parquet') 
  FORMAT 'CUSTOM' (FORMATTER='pxfwritable_import');

